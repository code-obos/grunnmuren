{
  "name": "@obosbbl/grunnmuren-icons-react",
  "version": "1.0.0",
  "description": "Grunnmuren's icon set",
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "type": "module",
  "repository": {
    "url": "https://github.com/code-obos/grunnmuren",
    "directory": "packages/icons-react"
  },
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "pnpm run build:make-react && pnpm run build:unbuild",
    "build:make-react": "node ./scripts/make-react-icons.mjs",
    "build:unbuild": "unbuild"
  },
  "peerDependencies": {
    "react": "^18"
  },
  "devDependencies": {
    "@obosbbl/grunnmuren-icons-svg": "workspace:*",
    "@svgr/core": "8.0.0",
    "@svgr/plugin-jsx": "8.0.1",
    "@svgr/plugin-svgo": "8.0.1",
    "fs-extra": "11.1.1"
  },
  "unbuild": {
    "rollup": {
      "esbuild": {
        "jsx": "automatic"
      }
    }
  }
}
