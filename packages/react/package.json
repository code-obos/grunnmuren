{
  "name": "@obosbbl/grunnmuren-react",
  "version": "2.0.0",
  "description": "Grunnmuren components in React",
  "repository": {
    "url": "https://github.com/code-obos/grunnmuren"
  },
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./button": {
      "types": "./dist/Button/Button.d.ts",
      "default": "./dist/Button/Button.mjs"
    },
    "./label": {
      "types": "./dist/label/index.d.ts",
      "default": "./dist/label/index.mjs"
    },
    "./radiogroup": {
      "types": "./dist/radiogroup/index.d.ts",
      "default": "./dist/radiogroup/index.mjs"
    },
    "./textfield": {
      "types": "./dist/textfield/index.d.ts",
      "default": "./dist/textfield/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild"
  },
  "dependencies": {
    "@obosbbl/grunnmuren-icons-react": "^1.0.0",
    "cva": "1.0.0-beta.1",
    "react-aria-components": "1.0.0-beta.2"
  },
  "peerDependencies": {
    "react": "^18"
  },
  "unbuild": {
    "entries": [
      "./src/Button/Button.tsx",
      "./src/label/index.ts",
      "./src/radiogroup/index.ts",
      "./src/textfield/index.ts"
    ],
    "declaration": true,
    "rollup": {
      "esbuild": {
        "jsx": "automatic"
      }
    }
  }
}
