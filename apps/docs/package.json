{
  "name": "@obosbbl/grunnmuren-docs",
  "version": "0.0.1",
  "private": true,
  "description": "Grunnmuren documentation",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "pnpm build:props && pnpm build:colors && pnpm build:tsc && pnpm build:assets && pnpm build:app",
    "build:app": "vite build",
    "build:assets": "mkdir -p public/resources/icons && cp -r node_modules/@obosbbl/grunnmuren-icons-svg/src/* public/resources/icons/",
    "build:props": "node extract-component-props.js && biome lint --write component-props.ts --files-max-size=8000000",
    "build:colors": "node extract-colors.js && biome format --write colors.ts",
    "build:tsc": "tsc",
    "dev": "vite dev --port 3000",
    "start": "node .output/server/index.mjs",
    "sanity:typegen": "pnpm sanity:typegen:extract && pnpm sanity:typegen:generate",
    "sanity:typegen:extract": "sanity schema extract",
    "sanity:typegen:generate": "sanity typegen generate"
  },
  "dependencies": {
    "@code-obos/sanity-auth": "1.4.3",
    "@obosbbl/grunnmuren-icons-react": "workspace:*",
    "@obosbbl/grunnmuren-icons-svg": "workspace:*",
    "@obosbbl/grunnmuren-react": "workspace:*",
    "@portabletext/react": "5.0.0",
    "@react-aria/utils": "3.31.0",
    "@sanity/asset-utils": "^2.3.0",
    "@sanity/client": "7.13.0",
    "@sanity/code-input": "6.0.3",
    "@sanity/image-url": "^1.2.0",
    "@sanity/table": "2.0.0",
    "@sanity/vision": "4.18.0",
    "@tanstack/nitro-v2-vite-plugin": "1.139.0",
    "@tanstack/react-router": "1.139.3",
    "@tanstack/react-start": "1.139.3",
    "@vitejs/plugin-react": "5.1.1",
    "cva": "1.0.0-beta.4",
    "groq": "4.18.0",
    "prism-react-renderer": "2.4.1",
    "react": "19.2.0",
    "react-aria-components": "1.13.0",
    "react-dom": "19.2.0",
    "react-element-to-jsx-string": "17.0.1",
    "react-live": "4.1.8",
    "sanity": "4.18.0",
    "use-debounce": "10.0.6"
  },
  "devDependencies": {
    "@obosbbl/grunnmuren-tailwind": "workspace:*",
    "@react-aria/optimize-locales-plugin": "1.1.5",
    "@tailwindcss/vite": "4.1.17",
    "@tanstack/react-router-devtools": "1.139.3",
    "react-docgen-typescript": "2.4.0",
    "tailwindcss": "4.1.17",
    "vite": "7.2.4",
    "vite-tsconfig-paths": "5.1.4"
  }
}
