{
  "compilerOptions": {
    "jsx": "react-jsx",
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "target": "ES2024",
    "skipLibCheck": true,
    "strictNullChecks": true,
    "noEmit": true,
    "paths": {
      "@/*": ["./src/*"],
      "@/sanity.types": ["./sanity.types"],
      "@/sanity.config": ["./sanity.config"],
      "@/component-props": ["./component-props"]
    }
  }
}
